<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    tools:context=".GRecyclerView">

    <ImageView
        android:id="@+id/btnNotificacion"
        android:layout_width="28dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/notificacion" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="88dp"
        android:layout_height="83dp"
        android:layout_marginStart="108dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnNotificacion"
        app:layout_constraintStart_toEndOf="@+id/btnNotificacion"
        app:layout_constraintTop_toTopOf="@+id/btnNotificacion"
        app:layout_constraintVertical_bias="0.301"
        app:srcCompat="@drawable/perfil" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginStart="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.283"
        app:srcCompat="@drawable/reunion" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/imageView14"
        app:srcCompat="@drawable/buscar" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:srcCompat="@drawable/mas_opciones" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_mensajes"
        android:layout_width="400dp"
        android:layout_height="196dp"
        android:clipToPadding="false"
        android:paddingVertical="8dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.021" />

    <ImageView
        android:id="@+id/imageEditar"
        android:layout_width="51dp"
        android:layout_height="36dp"
        android:layout_marginTop="196dp"
        app:layout_constraintEnd_toEndOf="@+id/rv_mensajes"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rv_mensajes"
        app:layout_constraintTop_toBottomOf="@+id/rv_mensajes"
        app:srcCompat="@drawable/editar" />

    <ImageView
        android:id="@+id/ImagenContactos"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/imageEditar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageEditar"
        app:layout_constraintTop_toBottomOf="@+id/imageEditar"
        app:srcCompat="@drawable/contacto" />

    <ImageView
        android:id="@+id/imagenLlamadas"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginEnd="132dp"
        app:layout_constraintBottom_toBottomOf="@+id/ImagenContactos"
        app:layout_constraintEnd_toStartOf="@+id/ImagenContactos"
        app:layout_constraintTop_toTopOf="@+id/ImagenContactos"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/telefono" />

    <ImageView
        android:id="@+id/imagenChats"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginEnd="156dp"
        app:layout_constraintBottom_toBottomOf="@+id/imagenLlamadas"
        app:layout_constraintEnd_toStartOf="@+id/imagenLlamadas"
        app:layout_constraintTop_toTopOf="@+id/imagenLlamadas"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chat" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Chats"
        app:layout_constraintEnd_toEndOf="@+id/imagenChats"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/imagenChats"
        app:layout_constraintTop_toBottomOf="@+id/imagenChats" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Llamadas"
        app:layout_constraintEnd_toEndOf="@+id/imagenLlamadas"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/imagenLlamadas"
        app:layout_constraintTop_toBottomOf="@+id/imagenLlamadas" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Contactos"
        app:layout_constraintEnd_toEndOf="@+id/ImagenContactos"
        app:layout_constraintHorizontal_bias="0.361"
        app:layout_constraintStart_toStartOf="@+id/ImagenContactos"
        app:layout_constraintTop_toBottomOf="@+id/ImagenContactos" />

    <Button
        android:id="@+id/secretBtn"
        android:layout_width="6dp"
        android:layout_height="21dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>